{
  "name": "thanos-wallet",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development run-s remove-dist watch-all",
    "build": "cross-env NODE_ENV=production run-s remove-dist copy-static deploy-all",
    "watch-all": "run-p *:watch",
    "deploy-all": "run-p *:deploy",
    "remove-dist": "rimraf build/",
    "ts:watch": "rollup -w -m --no-treeshake -c -d build/scripts/ ts/*.*",
    "ts:deploy": "rollup -c -d build/scripts/ ts/*.*",
    "pcss:watch": "postcss -d build/styles/ -m -w pcss/*.*",
    "pcss:deploy": "postcss -d build/styles/ pcss/*.*",
    "static:watch": "watch \"run-s copy-static\" static/",
    "manifest:watch": "watch \"run-s create-manifest\" manifest/",
    "manifest:deploy": "run-s create-manifest",
    "copy-static": "cp -r static/ build/",
    "create-manifest": "node ./bin/manifest.js create -o build/manifest.json manifest/manifest.template.js"
  },
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@fullhuman/postcss-purgecss": "^2.0.5",
    "@rollup/plugin-alias": "^3.0.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "@rollup/plugin-sucrase": "^3.0.0",
    "@surma/rollup-plugin-off-main-thread": "^1.1.1",
    "@taquito/taquito": "^6.0.0-beta.0",
    "@types/fontfaceobserver": "^0.0.6",
    "@types/invariant": "^2.2.31",
    "@types/node": "^13.5.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "autoprefixer": "^9.7.4",
    "babel-preset-react-app": "^9.1.0",
    "bignumber.js": "^9.0.0",
    "clsx": "^1.0.4",
    "cross-env": "^6.0.3",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "fontfaceobserver": "^2.1.0",
    "invariant": "^2.2.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "preact": "^10.2.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.29.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-terser": "^5.2.0",
    "sade": "^1.7.0",
    "tailwindcss": "^1.1.4",
    "typescript": "~3.7.5",
    "use-force-update": "1.0.7",
    "watch": "^1.0.2",
    "webextension-polyfill-ts": "^0.11.0",
    "wext-manifest": "^2.1.0",
    "wouter": "^2.4.0"
  }
}
