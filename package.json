{
  "private": true,
  "name": "@thanos/app",
  "version": "0.1.0",
  "scripts": {
    "start": "cross-env NODE_ENV=development run-s dist:remove watch",
    "build": "cross-env NODE_ENV=production run-s dist:remove build2",
    "build2": "run-p *:build",
    "watch": "run-p *:watch",
    "ts:watch": "rollup -w -m -c -f esm -d dist/scripts/ ts/*.*",
    "ts:build": "rollup -c -f esm -d dist/scripts/ ts/*.*",
    "pcss:watch": "postcss -d dist/styles/ -m -w pcss/*.*",
    "pcss:build": "postcss -d dist/styles/ pcss/*.*",
    "static:watch": "watch \"run-s static:copy\" static/",
    "static:build": "run-s static:copy",
    "static:copy": "cp -r static/ dist/",
    "dist:remove": "rimraf dist/"
  },
  "dependencies": {
    "clsx": "^1.0.4",
    "constate": "^1.2.0",
    "jdenticon": "^2.2.0",
    "preact": "10.0.0-rc.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "@types/chrome": "0.0.88",
    "@types/node": "12.7.4",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "4.3.5",
    "autoprefixer": "^9.6.1",
    "conseiljs": "0.2.6",
    "cross-env": "^5.2.1",
    "cssnano": "^4.1.10",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "postcss-import": "^12.0.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.20.3",
    "rollup-plugin-alias": "^2.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-sucrase": "^2.1.0",
    "rollup-plugin-terser": "^5.1.1",
    "tailwindcss": "^1.1.2",
    "watch": "^1.0.2"
  }
}
